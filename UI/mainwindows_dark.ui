    padding: 1px 1px 1px 5px;   /* 字体填衬 */
    color: rgb(0, 0, 0);    
	font: 700 10pt &quot;等线&quot;;    
	background-color: rgb(255, 255, 255);
}

/* 下拉后，整个下拉窗体样式 */
QComboBox QAbstractItemView {
    border: 1px solid rgb(65, 51, 156);   /* 整个下拉窗体的边框 */
    color: rgb(200, 200, 200);
	border-radius: 0px;
    background-color: rgb(13, 9, 36);   /* 整个下拉窗体的背景色 */
}
QComboBox QAbstractItemView::item {
	border:none;
    height: 25px; 
}
QComboBox QAbstractItemView::item:hover{
	border: none;	
	color: rgb(255, 0, 0);
	background-color: rgb(65, 49, 188);

}
QComboBox QAbstractItemView::item:selected{
	border: none;
}
/* 下拉箭头样式 */
 QComboBox::down-arrow {
	image: url(:/icon/icons/双下拉箭头.svg);
	width: 10px; /* 下拉箭头的宽度（建议与下拉框drop-down的宽度一致） */ 
	background: rgb(255, 255, 255); /* 下拉箭头的的背景色 */ 
	padding: 0px 0px 0px 0px; /* 上内边距、右内边距、下内边距、左内边距 */
 } 

/* 下拉框样式 */
QComboBox::drop-down {
   /* subcontrol-origin: padding;   /* 子控件在父元素中的原点矩形。如果未指定此属性，则默认为padding。 */
   /* subcontrol-position: top right;   /* 下拉框的位置（右上） */
    width: 20px;   /* 下拉框的宽度 */

    border-left-width: 3px;   /* 下拉框的左边界线宽度 */
    border-left-color: darkgray;   /* 下拉框的左边界线颜色 */
    border-left-style: solid;   /* 下拉框的左边界线为实线 */
    border-top-right-radius: 10px;   /* 下拉框的右上边界线的圆角半径（应和整个QComboBox右上边界线的圆角半径一致） */
    border-bottom-right-radius: 10px;   /* 同上 */
}
QComboBox:hover {
	border: 3px solid rgb(255, 85, 0);
}





/*右边的滑动条*/
 QScrollBar:vertical {
	border: none;
    background: transparent;
    width: 8px;
    margin: 17px 0 17px 0;
	border-radius: 0px;
 }
